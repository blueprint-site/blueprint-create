{
  "name": "@blueprint/frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:debug": "cross-env DEBUG=* vite",
    "build": "run-s clean:dist build:prod",
    "build:strict": "run-s clean:dist type-check build:prod",
    "build:prod": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "build:ci": "cross-env NODE_OPTIONS=--max-old-space-size=2048 vite build",
    "build:staging": "cross-env VITE_ENV=staging vite build",
    "clean:dist": "rimraf dist",
    "analyze": "cross-env ANALYZE=true vite build",
    "analyze:deps": "npx vite-bundle-visualizer",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 5",
    "lint:fix": "eslint . --fix --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "run-p type-check lint format:check",
    "preview": "vite preview",
    "preview:dist": "serve -s dist"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
